properties:
  file.name:
    displayName: Templates
views:
  - type: cards
    name: Table
    filters:
      and:
        - file.inFolder("03. templates")
        - categories == ["notion"]
    order:
      - file.name
      - Price
    sort:
      - property: Price
        direction: ASC
      - property: file.name
        direction: DESC
    columnSize:
      file.name: 319
    image: note.coverImage
    imageAspectRatio: 1
    cardSize: 200
