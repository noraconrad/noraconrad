properties:
  file.links:
    displayName: image
views:
  - type: cards
    name: Table
    filters:
      and:
        - file.inFolder("01. posts")
        - file.tags.contains("posts")
    sort:
      - property: date
        direction: DESC
      - property: title
        direction: ASC
    image: note.coverImage
    imageAspectRatio: 0.8
